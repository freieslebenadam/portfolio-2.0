{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@context": ["context"],
      "@context/*": ["context/*"],
      "@hooks": ["hooks"],
      "@styles/*": ["styles/*"],
      "@types": ["ts/types"],
      "@interfaces": ["ts/interfaces"],
      "@enums": ["ts/enums"],
      "@lib": ["lib"],
      "@lib/*": ["lib/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
